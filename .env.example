# Environment Configuration Example
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# ==============================================================================
# ENVIRONMENT SETTINGS
# ==============================================================================
ENV=Development
CLIENT_ID=1
HOSTED_URL=https://localhost:44357/
IDENTITY_SERVER_URL=https://localhost:44349
FRONTEND_URL=https://localhost:3000

# ==============================================================================
# ADMIN & NOTIFICATION SETTINGS
# ==============================================================================
# Email address of the super administrator
SUPER_ADMIN_EMAIL=your_admin@example.com

# Comma-separated list of emails to receive system notifications
NOTIFICATION_EMAILS=admin1@example.com,admin2@example.com,admin3@example.com

# ==============================================================================
# DATABASE CONNECTION
# ==============================================================================
# Format: Server=YOUR_SERVER;Database=YOUR_DB;User Id=YOUR_USER;Password=YOUR_PASSWORD;...
CONNECTION_STRING=Server=YOUR_SERVER;Database=YOUR_DATABASE;User Id=YOUR_USER;Password=YOUR_PASSWORD;integrated security=False;MultipleActiveResultSets=False;App=EntityFramework

# ==============================================================================
# AWS CREDENTIALS
# ==============================================================================
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_DISTRIBUTION_ID=your_cloudfront_distribution_id
AWS_AMPLIFY_APP_ID=your_amplify_app_id
AWS_BRANCH_NAME=your_branch_name
BLOB_URL=https://your-bucket.s3.amazonaws.com/

# ==============================================================================
# SENDGRID EMAIL SERVICE
# ==============================================================================
SENDGRID_API_KEY=your_sendgrid_api_key
MAIL_FROM=your_email@example.com

# ==============================================================================
# SEO & KEYWORD RANKING
# ==============================================================================
DATAFORSEO_LOGIN=your_dataforseo_email
DATAFORSEO_PASSWORD=your_dataforseo_password

# ==============================================================================
# API2PDF
# ==============================================================================
API2PDF_KEY=your_api2pdf_key

# ==============================================================================
# APPLICATION INSIGHTS
# ==============================================================================
APPLICATION_INSIGHTS_INSTRUMENTATION_KEY=your_instrumentation_key

# ==============================================================================
# GO HIGH LEVEL
# ==============================================================================
GOHIGHLEVEL_API_KEY=your_gohighlevel_api_key

# ==============================================================================
# MAILCHIMP
# ==============================================================================
MAILCHIMP_API_KEY=your_mailchimp_api_key
MAILCHIMP_LIST_ID=your_mailchimp_list_id
MAILCHIMP_CLIENT_ID=your_mailchimp_client_id
MAILCHIMP_SECRET_ID=your_mailchimp_secret_id
MAILCHIMP_REDIRECT_URL=html/auth-redirect.html

# ==============================================================================
# GOOGLE ADS & ANALYTICS
# ==============================================================================
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_DEVELOPER_TOKEN=your_google_developer_token
GOOGLE_API_KEY=your_google_api_key

# ==============================================================================
# FACEBOOK ADS
# ==============================================================================
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret

# ==============================================================================
# LINKEDIN ADS
# ==============================================================================
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_SECRET_ID=your_linkedin_secret_id

# ==============================================================================
# STRIPE PAYMENT
# ==============================================================================
STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_TAX_ID=your_stripe_tax_id

# Stripe Product Price IDs
STRIPE_PRICE_CUSTOM_MONTHLY=price_your_custom_monthly_id
STRIPE_PRICE_CUSTOM_YEARLY=price_your_custom_yearly_id
STRIPE_PRICE_AGENCY_MONTHLY=price_your_agency_monthly_id
STRIPE_PRICE_AGENCY_YEARLY=price_your_agency_yearly_id
STRIPE_PRICE_STARTUP_MONTHLY=price_your_startup_monthly_id
STRIPE_PRICE_STARTUP_YEARLY=price_your_startup_yearly_id
STRIPE_PRICE_PROFESSIONAL_MONTHLY=price_your_professional_monthly_id
STRIPE_PRICE_PROFESSIONAL_YEARLY=price_your_professional_yearly_id

# Stripe Coupon Codes
STRIPE_COUPON_20_PERCENT=your_20_percent_coupon_code
STRIPE_COUPON_50_PERCENT=your_50_percent_coupon_code

# Marketplace Product ID
MARKETPLACE_PRODUCT_ID=prod_your_marketplace_product_id

# ==============================================================================
# MICROSOFT ADS
# ==============================================================================
MICROSOFT_CLIENT_ID=your_microsoft_client_id
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret
MICROSOFT_DEVELOPER_TOKEN=your_microsoft_developer_token

# ==============================================================================
# OPENAI / CHATGPT
# ==============================================================================
CHATGPT_API_KEY=your_chatgpt_api_key

# ==============================================================================
# AZURE WEBJOBS (if using Azure WebJobs)
# ==============================================================================
AZURE_WEBJOBS_DASHBOARD_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=YOUR_ACCOUNT;AccountKey=YOUR_KEY;EndpointSuffix=core.windows.net
AZURE_WEBJOBS_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=YOUR_ACCOUNT;AccountKey=YOUR_KEY;EndpointSuffix=core.windows.net

# ==============================================================================
# URL CONFIGURATIONS
# ==============================================================================
RETURN_URL_SUPERADMIN_AGENCY_VERIFICATION=superAdminAgencyActivation
RETURN_URL_EMAIL_VERIFICATION=emailVerification
RESET_URL=resetPassword
